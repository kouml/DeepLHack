{
  "name": "DeepLHack",
  "version": "0.0.1",
  "description": "This app is able to use DeepL from paper/thesis without modifying",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild",
    "pack": "electron-packager . deeplhack --platform=win32 --arch=x64",
    "pack_all": "electron-packager . deeplhack --ignore=node_modules --all"
  },
  "repository": "https://github.com/kouml/DeepLHack/",
  "keywords": [
    "DeepL",
    "DeepLHack"
  ],
  "author": "kouml",
  "license": "MIT",
  "dependencies": {
    "electron": "^9.0.0"
  },
  "devDependencies": {
    "electron-packager": "^15.0.0",
    "electron-rebuild": "^1.11.0"
  }
}
